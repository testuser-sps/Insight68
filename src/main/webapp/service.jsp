<div class="container">
	<div class="row mx-5">
		<div class="col-12 text-center my-4">

			<h5 class="black py-2">
				Please fill in fields below. We will get back to you shortly. Thanks.
			</h5>


			<form id="serviceFrom">
				<div class="row pt-3 mx-5 px-5">
					<div class="col-md-6">
						<div class="form-group">
							<input type="text" class="form-control" id="sffirstName"
								name="sffirstName" placeholder="First Name" minlength="3"
								maxlength="20"  autocomplete="off" />
						</div>
					</div>
					<div class="col-md-6">
						<div class="form-group">
							<input type="text" class="form-control" id="sflastName"
								name="sflastName" placeholder="Last Name" minlength="1"
								maxlength="20" autocomplete="off" />
						</div>
					</div>
					<div class="col-md-6">
						<div class="form-group">
							<input type="email" class="form-control" id="sfemail"
								name="sfemail" placeholder="Email Address" minlength="6"
								maxlength="75"  autocomplete="off" />
						</div>
					</div>
					<div class="col-md-6">
						<div class="form-group">
							<input type="text" class="form-control" id="sfsubject"
								name="sfsubject" placeholder="Subject" minlength="5"
								maxlength="100"  autocomplete="off" />
						</div>
					</div>
					<div class="col-md-6">
						<div class="form-group" id="custom-select">
							<select class="form-control" id="sfControlSelect1" name="sfControlSelect1"
								required>
								<option value="">Select Product</option>
								<option value="Analytics, Dashboards & Reports">Analytics, Dashboards & Reports</option>
								<option value="Quality Management">Quality Management</option>
								<option value="Inventory Management">Inventory Management</option>
								<option value="Study Management">Study Management - IRT</option>
								<option value="Logistics">Logistics</option>
								<option value="Scheduler">Scheduler</option>
								<option value="Manufacturing">Manufacturing</option>
								<option value="Pricing">Pricing</option>
								<option value="Others">Others</option>
							</select>
						</div>
					</div>

					<div class="col-md-6">
						<div class="form-group">
							<input type="text" class="form-control" id="sfsocialmediaid"
								name="sfsocialmediaid" 
								placeholder="Skype ID" 
								autocomplete="off" />
						</div>
					</div>
					<div class="col-md-12">
						<div class="form-group">
							<input type="text" class="form-control" id="swhatsappnumber"
								name="swhatsappnumber" minlength="10" maxlength="10"
								placeholder="WhatsApp Number" 
								autocomplete="off" />
						</div>
					</div>
					<div class="col-md-12">
						<div class="form-group">
							<textarea placeholder="Message" id="sfmessage" rows="5" name="sfmessage"
								class="form-control" minlength="10" maxlength="500" 
								autocomplete="off"></textarea>
						</div>
					</div>
				</div>
				<div class="py-4">
					<button class="btn btn-navy text-uppercase font-weight-bold mb-3"
						type="button" id="serviceFromBtn">Submit</button>
				</div>

			</form>

		</div>
	</div>
</div>

